{
  "grammars": [
    {
      "name": "trident",
      "camelcase": "Trident",
      "scope": "source.trident",
      "path": ".",
      "file-types": [
        "tri"
      ],
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for the Trident ZK language",
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-trident"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
